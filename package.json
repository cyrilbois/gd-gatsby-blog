{
  "name": "gatsby-starter-drunkenblog",
  "description": "A Gatsby Blog",
  "version": "1.0.0",
  "author": "David Konsumer <konsumer@jetboystudio.com>",
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "catch-links": "^2.0.1",
    "gatsby": "^1.8.12",
    "history": "^4.6.3",
    "just-intersect": "^1.1.20",
    "lodash": "^4.13.1",
    "moment": "^2.14.1",
    "react": "^15.2.1",
    "react-disqus-thread": "^0.4.0",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.2.1",
    "react-ga": "^2.1.1",
    "react-responsive-grid": "^0.3.3",
    "react-router": "^4.1.2",
    "react-typography": "^0.16.1",
    "safe-access": "^0.1.0",
    "slugify": "^1.2.1",
    "typography": "^0.16.0",
    "typography-theme-wordpress-2016": "^0.15.10",
    "underscore.string": "^3.2.3"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^7.1.2",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "gh-pages": "^1.0.0",
    "image-webpack-loader": "^3.3.1",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.0.6",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.10",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5"
  },
  "homepage": "http://blog.jetboystudio.com",
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konsumer/blog.jetboystudio.com.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-links && gh-pages -d public"
  },
  "eslintConfig": {
    "extends": "eslint-config-airbnb",
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-console": [
        0
      ],
      "func-names": [
        0
      ],
      "semi": [
        2,
        "never"
      ],
      "no-extra-semi": [
        2
      ],
      "space-before-function-paren": [
        2,
        "always"
      ],
      "no-else-return": [
        0
      ],
      "space-infix-ops": [
        0
      ],
      "react/prefer-es6-class": [
        0
      ],
      "react/prefer-stateless-function": [
        0
      ],
      "import/no-unresolved": [
        0
      ],
      "global-require": [
        0
      ]
    },
    "globals": {
      "__PREFIX_LINKS__": true
    }
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
